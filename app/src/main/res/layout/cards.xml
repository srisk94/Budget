<?xml version="1.0" encoding="utf-8"?>
<com.srima.bb.TACGridView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:columnWidth="@dimen/cardWidth"
    android:horizontalSpacing="@dimen/cardSpacing"
    android:verticalSpacing="@dimen/cardSpacing"
    android:paddingLeft="@dimen/cardSpacing"
    android:paddingRight="@dimen/cardSpacing"
    android:stretchMode="columnWidth"
    android:numColumns="auto_fit"
    android:listSelector="#00000000"
    android:id="@+id/grid"
/>
